vsim work.nstalls_count
# vsim work.nstalls_count 
# Start time: 20:00:56 on Dec 03,2017
# Loading std.standard
# Loading std.textio(body)
# Loading ieee.std_logic_1164(body)
# Loading ieee.numeric_std(body)
# Loading work.basic
# Loading work.nstalls_count(behave)
# Loading work.mux_2to1_nbits(behave)
# Loading work.dregister(behave)
add wave -position insertpoint  \
sim:/nstalls_count/nstalls \
sim:/nstalls_count/clk \
sim:/nstalls_count/stall_bit \
sim:/nstalls_count/adder_mux_out \
sim:/nstalls_count/mux_select \
sim:/nstalls_count/reg_out \
sim:/nstalls_count/mux_out \
sim:/nstalls_count/add_out
force -freeze sim:/nstalls_count/nstalls 010 0
force -freeze sim:/nstalls_count/clk 0 0, 1 {50 ps} -r 100
force -freeze sim:/nstalls_count/stall_bit 1 0, 0 {50 ps} -r 100
run
force -freeze sim:/nstalls_count/stall_bit 0 0
run
run
run
restart
force -freeze sim:/nstalls_count/nstalls 010 0
force -freeze sim:/nstalls_count/clk 1 0, 0 {50 ps} -r 100
force -freeze sim:/nstalls_count/stall_bit 0 0
run
run
force -freeze sim:/nstalls_count/stall_bit 1 0
run
run
run
run
run
